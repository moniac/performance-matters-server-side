<% include ./partials/head %>

	<% if (data) { %>
	<section>
		<ul id='myUL'>
			<% data.forEach((building) => { %>
				<li>
					<a href="/building/<%-building.id%>">
						<%-building.name%>
					</a>
				</li>
				<%}) %>
		</ul>
	</section>
		<%	} else { %>
			<h2>Sorry, there is no data available!</h2>
			<%	} %>


<script async defer>
	let data = <%- JSON.stringify( data )  %>

</script>

<script src='./scripts/bundle.js'></script>

<% include ./partials/foot %>